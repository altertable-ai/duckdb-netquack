# name: test/sql/extract_domain.test
# description: test netquack extension extract_domain function
# group: [netquack]

require netquack

query I
SELECT extract_domain('domain.com');
----
domain.com

query I
SELECT extract_domain('a.domain.com');
----
domain.com

query I
SELECT extract_domain('domain.com.ac');
